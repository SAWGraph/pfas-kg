### included here for reference purposes only in Protege; defined in Geosparql
###  http://www.opengis.net/ont/geosparql#Feature
geo:Feature rdf:type owl:Class .

### included here for reference purposes only in Protege; defined in Geosparql
###  http://www.opengis.net/ont/geosparql#Geometry
geo:Geometry rdf:type owl:Class .

pfas:QuantitationLimit rdf:type owl:Class ;
    rdfs:subClassOf coso:ResultQualifier.

pfas:MethodDetectionLimit rdf:type owl:Class ;
    rdfs:subClassOf coso:ResultQualifier.


pfas:SinglePFASConcentrationMeasurement rdf:type owl:Class ;
    rdfs:subClassOf coso:SingleContaminantMeasurement.

pfas:AnalysisMethod rdf:type owl:Class . 
					   

pfas:PFAS-ConcentrationMeasurement rdf:type owl:Class ;
    rdfs:subClassOf coso:ContaminantMeasurement .

pfas:PFAS-VolumeMeasurement  rdf:type owl:Class;
    rdfs:subClassOf coso:ContaminantVolumeMeasurement;
    rdfs:subClassOf coso:ContaminantMeasurement.

pfas:AggregatePFASAmountMeasurement rdf:type owl:Class;
    rdfs:subClassOf coso:AggregateContaminantMeasurement .

pfas:PFAS-ContaminantObservation rdf:type owl:Class ;
    rdfs:subClassOf coso:SampleContaminantObservation .

pfas:PFAS-ReleaseContaminantObservation rdf:type owl:Class ;
    rdfs:subClassOf coso:ReleaseContaminantObservation .

pfas:PFAS-SampleContaminantObservation rdf:type owl:Class ;
    rdfs:subClassOf coso:SampleContaminantObservation .
					   
