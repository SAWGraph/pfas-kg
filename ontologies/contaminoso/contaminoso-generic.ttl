###  http://qudt.org/schema/qudt/Quantity
qudt:Quantity rdf:type owl:Class .


###  http://qudt.org/schema/qudt/QuantityKind
qudt:QuantityKind rdf:type owl:Class .


###  http://sawgraph.spatialai.org/v1/contaminoso#Analyte
coso:Analyte rdf:type owl:Class ;
             rdfs:subClassOf sosa:ObservableProperty ;
             rdfs:label "Analyte" .


###  http://sawgraph.spatialai.org/v1/contaminoso#ContaminantMeasurement
coso:ContaminantMeasurement rdf:type owl:Class ;
                            rdfs:subClassOf qudt:Quantity ,
                                            sosa:Result ;
                            rdfs:label "Contaminant Measurement" .


###  http://sawgraph.spatialai.org/v1/contaminoso#ContaminantObservation
coso:ContaminantObservation rdf:type owl:Class ;
                            rdfs:subClassOf sosa:Observation ;
                            rdfs:label "Contaminant Observation" .


###  http://sawgraph.spatialai.org/v1/contaminoso#ContaminantSource
coso:ContaminantSource rdf:type owl:Class ;
                       rdfs:subClassOf coso:Feature ;
                       rdfs:label "Contaminant Source" .


###  http://sawgraph.spatialai.org/v1/contaminoso#Facility
coso:Facility rdf:type owl:Class ;
              rdfs:subClassOf coso:ContaminantSource ;
              rdfs:comment "Denotes an entity (place, building, equipment, or service) that is a source of environmental concern." ;
              rdfs:label "Facility" ;
              skos:example "The Facility Registry System (FRS) is a centrally managed Environmental Protection Agency (EPA) database that identifies facilities, sites or places of environmental interest in the United States." .


###  http://sawgraph.spatialai.org/v1/contaminoso#Feature
coso:Feature rdf:type owl:Class ;
             rdfs:subClassOf geo:Feature ,
                             sosa:FeatureOfInterest ;
             rdfs:label "COSO Feature" .


###  http://sawgraph.spatialai.org/v1/contaminoso#HazardEvent
coso:HazardEvent rdf:type owl:Class ;
                 rdfs:subClassOf coso:ContaminantSource ;
                 rdfs:comment "Denotes a hazardous incident that may be a spill or release of chemicals, radioactive materials or biological materials inside a building or to the environment." ;
                 rdfs:label "Hazard Event" .


###  http://sawgraph.spatialai.org/v1/contaminoso#SampleFeature
coso:SampleFeature rdf:type owl:Class ;
                   rdfs:subClassOf coso:Feature ;
                   rdfs:label "Sample Feature" .


###  http://sawgraph.spatialai.org/v1/contaminoso#SamplePoint
coso:SamplePoint rdf:type owl:Class ;
                 rdfs:subClassOf coso:Feature ;
                 rdfs:comment "Refers to a specific location or site within a sample feature where samples are collected for testing or analysis to determine the presence and concentration of contaminants. These sample points are strategically chosen based on various factors such as suspected contamination sources, environmental pathways, and regulatory requirements. The samples collected from these points provide crucial data for understanding the extent of contamination, assessing potential risks to human health and the environment, and formulating remediation strategies." ;
                 rdfs:label "Sample Point" .


