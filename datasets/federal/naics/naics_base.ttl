###  http://sawgraph.spatialai.org/v1/fio#NAICS-Industry-Code
naics:NAICS-Industry-Code rdf:type owl:Class ;
                        rdfs:subClassOf naics:NAICS-IndustryGroup .


###  http://sawgraph.spatialai.org/v1/fio#NAICS-IndustryGroup
naics:NAICS-IndustryGroup rdf:type owl:Class ;
                        rdfs:subClassOf naics:NAICS-Subsector .

###  http://sawgraph.spatialai.org/v1/fio#NAICS-Sector
naics:NAICS-Sector rdf:type owl:Class ;
                 rdfs:subClassOf fio:Industry-Code .


naics:NAICS-Sector-32 rdfs:subClassOf naics:NAICS-Sector ;
                    rdfs:label "Manufacturing" ;
					owl:equivalentClass naics:NAICS-Sector-31.

naics:NAICS-Sector-33 rdfs:subClassOf naics:NAICS-Sector ;
                    rdfs:label "Manufacturing" ;
					owl:equivalentClass naics:NAICS-Sector-31.

naics:NAICS-Sector-45 rdfs:subClassOf naics:NAICS-Sector;
					rdfs:label "Retail Trade";
					owl:equivalentClass naics:NAICS-Sector-44.

naics:NAICS-Sector-49 rdfs:subClassOf naics:NAICS-Sector;
					rdfs:label "Transportation and Warehousing";
					owl:equivalentClass naics:NAICS-Sector-48.

naics:NAICS-Sector-48 rdfs:subClassOf naics:NAICS-Sector.
naics:NAICS-Sector-44 rdfs:subClassOf naics:NAICS-Sector.
naics:NAICS-Sector-33 rdfs:subClassOf naics:NAICS-Sector.
naics:NAICS-Sector-31 rdfs:subClassOf naics:NAICS-Sector.


###  http://sawgraph.spatialai.org/v1/fio#NAICS-Subsector
naics:NAICS-Subsector rdf:type owl:Class ;
                    rdfs:subClassOf naics:NAICS-Sector .

##Individuals
naics:NAICS-Sector-32 a naics:NAICS-Sector;
    owl:sameAs naics:NAICS-Sector-31,
                naics:NAICS-Sector-33.

naics:NAICS-Sector-33 a naics:NAICS-Sector;
    owl:sameAs naics:NAICS-Sector-31,
                naics:NAICS-Sector-32.

naics:NAICS-Sector-45 a naics:NAICS-Sector;
    owl:sameAs naics:NAICS-Sector-44.

naics:NAICS-Sector-49 a naics:NAICS-Sector;
    owl:sameAs naics:NAICS-Sector-48. 