###  http://sawgraph.spatialai.org/v1/us-ghg#Chemical
us_ghg:Chemical rdf:type owl:Class ;
                    rdfs:subClassOf coso:Substance .

us_ghg:GHG-Chemical-Collection rdf:type owl:Class ;
                    rdfs:subClassOf coso:SubstanceCollection .


###  http://sawgraph.spatialai.org/v1/us-ghg#Measurement
us_ghg:Measurement rdf:type owl:Class ;
                       rdfs:subClassOf coso:AggregateContaminantMeasurement .


###  http://sawgraph.spatialai.org/v1/us-ghg#ReleaseObservation
us_ghg:ReleaseObservation rdf:type owl:Class ;
                              rdfs:subClassOf coso:ContaminantReleaseObservation .

### Feature
us_ghg:Facility rdf:type owl:Class ;
    rdfs:subClassOf coso:MonitoredFeature, epa-frs:FRS-Facility. 

us_ghg:GHGSubpart rdf:type owl:Class ;
    rdfs:subClassOf coso:ObservationAnnotation. 

epa-frs:FRS-Facility rdf:type owl:Class;
    rdfs:subClassOf geo:Feature. 

#################################################################
# Object Properties
#################################################################

us_ghg:ghgSubpart rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf coso:observationAnnotation ;
    rdfs:range us_ghg:GHGSubpart . 




