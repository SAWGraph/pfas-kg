@prefix coso: <http://sawgraph.spatialai.org/v1/contaminoso#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix us_wqp: <http://sawgraph.spatialai.org/v1/us-wqp#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

us_wqp:Blank-AdjustedMethodDetectLimit rdfs:label "Blank-adjusted method detect limit"^^xsd:string ;
    rdfs:comment "Blank-adjusted method detect limit"^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:Blank-AdjustedReportingLimit rdfs:label "Blank-adjusted reporting limit"^^xsd:string ;
    rdfs:comment "Greater than or equal to two times the blank-adjusted method detect limit. The probability of falsely reporting a non-detection blank-adjusted limit is predicted to be less than or equal to 1 percent."^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:CensoringLevel rdfs:label "Censoring level"^^xsd:string ;
    rdfs:comment "Lower or upper value at which the result is censored"^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:ContractDetectionLimit rdfs:label "Contract Detection Limit"^^xsd:string ;
    rdfs:comment "Minimum level of detection acceptable under the contract Statement of Work (SOW). However, laboratory-derived IDLs (adjusted for sample size, dilution and moisture) are used for reporting limits"^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:ContractQuantitationLimit rdfs:label "Contract Quantitation Limit"^^xsd:string ;
    rdfs:comment "- Minimum level of reliable quantitation acceptable under the contract Statement of Work (SOW)."^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:DailyDetectionLimit rdfs:label "Daily detection limit"^^xsd:string ;
    rdfs:comment "Daily detection limit"^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:DetectionLimitByDqcalc rdfs:label "Detection limit by DQCALC"^^xsd:string ;
    rdfs:comment "Lowest concentration, with 90% confidence, will be exceeded no more than 1 percent when a blank sample is measured. Detection limit by DLDQC is the critical level \"Lc\" by ASTM D6091, nearly equal MDL."^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:DrinkingWaterMaximum rdfs:label "Drinking Water Maximum"^^xsd:string ;
    rdfs:comment "Maximum Contaminant Levels (MCLs) is the legal threshold limit on the amount of a substance that is allowed in public water systems under the Safe Drinking Water Act."^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:EstimatedDetectionLevel rdfs:label "Estimated Detection Level"^^xsd:string ;
    rdfs:comment "Estimated Detection Level -- Minimum concentration required to produce a specified signal-to-noise (S/N) ratio."^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:EstimatedQuantitationLimit rdfs:label "Estimated Quantitation Limit"^^xsd:string ;
    rdfs:comment "Lowest concentration that can be reliably achieved within specified limits of precision and accuracy during routine lab operations. EQLs normally are arbitrarily set rather than explicitly determined."^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:FieldHoldingTimeLimit rdfs:label "Field Holding Time Limit"^^xsd:string ;
    rdfs:comment "Performed in field. Samples are only representative of the area they were taken from for a specific length of time before sample preparation or analysis must begin. This is defined as \"holding time.\""^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:InstrumentDetectionLevel rdfs:label "Instrument Detection Level"^^xsd:string ;
    rdfs:comment "Instrument Detection Level(IDL)--Lowest concentration that can be detected by an instrument without correction for the effects of sample matrix or method-specific parameters such as sample preparation"^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:InterimReportingLevel rdfs:label "Interim Reporting Level"^^xsd:string ;
    rdfs:comment "Interim Reporting Level - a temporary reporting level used for new or custom schedules when a LRL has not yet been established"^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:LaboratoryHoldingTimeLimit rdfs:label "Laboratory Holding Time Limit"^^xsd:string ;
    rdfs:comment "Performed in Lab. Samples are only representative of the area they were taken from for a specific length of time before sample preparation or analysis must begin. This is defined as  \"holding time.\""^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:LaboratoryReportingLevel rdfs:label "Laboratory Reporting Level"^^xsd:string ;
    rdfs:comment "Laboratory Reporting Level - formerly referred to as Non-Detection Value"^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:LongTermMethodDetectionLevel rdfs:label "Long Term Method Detection Level"^^xsd:string ;
    rdfs:comment "is a modification of the defintion of the method detection limit (MDL). It includes a more thorough capture of laboratory variability, and is calculated using three different possible data assessments"^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:Lower95percentConfidenceLimit rdfs:label "Lower 95% Confidence Limit"^^xsd:string ;
    rdfs:comment "Lower limit of 95% confidence limit, the below point estimate within which the true value "^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:LowerLimitOfDetection rdfs:label "Lower limit of detection"^^xsd:string ;
    rdfs:comment "- Generally the same as Instrument Detection Level (IDL). IDLs are explicitly determined and generally defined as three times the standard deviation of the mean noise level."^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:LowerQuantitationLimit rdfs:label "Lower Quantitation Limit"^^xsd:string ;
    rdfs:comment "Lower Limit of Quantitation(LLQ) Generally the same as EQL. "^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:LowerReportingLimit rdfs:label "Lower Reporting Limit"^^xsd:string ;
    rdfs:comment "Lower Reporting Limit"^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:MeasurementUncertainty rdfs:label "Measurement Uncertainty"^^xsd:string ;
    rdfs:comment "A quantitative expression of error that results from incomplete knowledge or information about a parameter or value"^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:MethodDetectionLevel rdfs:label "Method Detection Level"^^xsd:string ;
    rdfs:comment "Synonymous with Method Detection Limit (MDL), is the minimum concentration of a substance that can be measured and reported with a 99% confidence that the analyte concentration is greater than zero."^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:MinimumReportingLevel rdfs:label "Minimum Reporting Level"^^xsd:string ;
    rdfs:comment "Minimum Reporting Level is the smallest measured concentration of a constituent that can be reliably measured using a given analytical method"^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:PracticalQuantitationLimit rdfs:label "Practical Quantitation Limit"^^xsd:string ;
    rdfs:comment "Practical Quantitation Limit (PQL)"^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:ReportingLimit rdfs:label "Reporting limit"^^xsd:string ;
    rdfs:comment "Reporting limit"^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:ReportingLimitByDqcalc rdfs:label "Reporting limit by DQCALC"^^xsd:string ;
    rdfs:comment "Greater than or equal to two times the detection limit by DQCALC, where the detection limit by DQCALC is the critical level \"Lc\" by ASTM D6091. DQCALC software used to determine limits (ASTM D7510-10)"^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:RequiredDetectionLimit rdfs:label "Required detection limit"^^xsd:string ;
    rdfs:comment "New term from the Federal Register ... to be investigated"^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:Sample-SpecificCriticalLevel rdfs:label "Sample-specific critical level"^^xsd:string ;
    rdfs:comment "The calculated and reported value below which a radiochemistry result is considered a non-detection. "^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:Sample-SpecificMinDetectConc rdfs:label "Sample-specific min detect conc"^^xsd:string ;
    rdfs:comment "The smallest concentration that an instrument can reliably detect for a specific sample."^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:Sample-SpecificQuantitationLimit rdfs:label "Sample-Specific Quantitation Limit"^^xsd:string ;
    rdfs:comment "The largest or smallest concentration that an instrument can reliably detect for a specific sample."^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:SampleDetectionLimit rdfs:label "Sample Detection Limit"^^xsd:string ;
    rdfs:comment "Sample Detection Limit (SDL) - The MDL adjusted to reflect sample-specific actions such as dilution or use of smaller aliquot sizes, or to report results on a dry-weight basis."^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:SpecifiedInWorkplan rdfs:label "Specified in workplan"^^xsd:string ;
    rdfs:comment "Specified in workplan... requested by GLENDA"^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:StatisticalUncertainty rdfs:label "Statistical Uncertainty"^^xsd:string ;
    rdfs:comment "That component of uncertainty which arises from imprecision. www.iaea.org/OurWork/ST/NE/NEFW/WTS-Networks/IDN/idnfiles/Wkp_on_Development_WM,WasteCharacterization_Clearance/Day3/12.Statistics_GREY.pdf"^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:SystematicUncertainty rdfs:label "Systematic Uncertainty"^^xsd:string ;
    rdfs:comment "That component of uncertainty which arises from biases. www.iaea.org/OurWork/ST/NE/NEFW/WTS-Networks/IDN/idnfiles/Wkp_on_Development_WM,WasteCharacterization_Clearance/Day3/12.Statistics_GREY.pdf"^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:TaxonomicLossThreshold rdfs:label "Taxonomic Loss Threshold"^^xsd:string ;
    rdfs:comment "Taxonomic Loss Threshold"^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:Upper95percentConfidenceLimit rdfs:label "Upper 95% Confidence Limit"^^xsd:string ;
    rdfs:comment "Upper limit of 95% confidence limit, the above point estimate within which the true value "^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:UpperCalibrationLimit rdfs:label "Upper Calibration Limit"^^xsd:string ;
    rdfs:comment "Highest concentration that can be reliably measured. The highest calibration standard in the lab's initial calibration curve adjusted for initial sample volume or weight"^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:UpperQuantitationLimit rdfs:label "Upper Quantitation Limit"^^xsd:string ;
    rdfs:comment "The largest concentration that an instrument can reliably detect."^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:UpperReportingLimit rdfs:label "Upper Reporting Limit"^^xsd:string ;
    rdfs:comment "The largest concentration that can be reliably reported."^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

us_wqp:WaterQualityStandardOrCriteria rdfs:label "Water Quality Standard or Criteria"^^xsd:string ;
    rdfs:comment "Provisions of state, territorial, tribal or federal law approved by EPA that describe the desired condition of aÂ waterÂ body and the means by which that condition will be protected or achieved."^^xsd:string ;
    rdfs:subClassOf coso:ResultQualifier .

